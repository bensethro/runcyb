/* Links & Navigation
-------------------------------------------------------------- */
a {
	color: #333;
	text-decoration: none;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}

a:hover {
	color: #80bd01;
}

.site-header.small .nav ul {
	margin-top: 14px;
}

.nav .current_page_item a {color: #156736;}

.nav ul {
	float: right;
	list-style-type: none;
	margin: 26px 0 0;
}

.nav ul li {
	display: inline-block;	
	background: none;
	text-indent: 0;
	position: relative;
}

.nav ul li:first-child {
	margin-left: 0;
}

.nav ul li a {
	font-family: 'Roboto Slab', sans-serif;
	margin: 0 0 0 20px;
	font-size: 16px;
	font-weight: 600;
	padding: 0;
	text-decoration: none;
	display: inline-block;
	color: #333;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}

.nav ul li a:hover, .nav ul li a.active {
	color: rgba(58,180,73,1);
}

.nav ul li ul.sub-menu {
	display: none;
	position: absolute;
	top: 10px;
	left: 16px;
	width: 250px;
	border:2px solid #CCC;
	padding: 5px 10px;
	background: #ffffff;
}

.nav ul li:hover ul.sub-menu {
	display: block;
}

.nav ul li ul.sub-menu li {
	width:100%;
	float:left;
}

.nav ul li ul.sub-menu li a {
	margin:0;
	padding: 2px 2px;
	text-decoration: none;
	display: block;
	color: #333;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
	font-family: 'Roboto', sans-serif;
	font-weight: 300;
	font-size: 14px;
	border-bottom:dotted #999 1px;
}

.nav ul li ul.sub-menu li:last-child a {border-bottom:none;}

.nav ul li ul.sub-menu li a:hover, .nav ul li ul.sub-menu li a.active {
	color: rgba(58,180,73,1);
}

.toggle-menu { display: none; }


