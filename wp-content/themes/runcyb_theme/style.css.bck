/* MEDIA QUERIES =============================================
-------------------------------------------------------------- */


/* 740px
-------------------------------------------------------------- */

@media screen and (max-width: 740px) {

	body {
		background: #fff;
	}
	
	ul.tabs {
	   display:none;
	   visability:hidden;
	}

	div.tabcontents #view1,#view2,#view3,#view4,#view5 {
	    display:block!important;
		border-bottom:6px #999 solid!important;
		padding-bottom:20px;
		margin-bottom: 40px!important;
	}

	div.tabcontents {
	    margin-top:30px;
		border:none;
		margin:0px;
		padding:0px;
	}

	.tabcontents hr {
		margin-bottom: 0;
	}

	.tabcontents img.alignright {
		margin: 0 0 20px;
		float: none;
		clear: both!important;
		display: block;
	}

}

/* 930px
-------------------------------------------------------------- */
@media screen and (max-width: 929px) {

	.site-header, .site-header.large, .site-header.small {
		padding: 10px 0 10px 0;
		border-bottom: solid #CCC 1px;
	}

	.site-header.small .logo, .site-header.large .logo {
		width: 240px;
	}

	.toggle-menu {
		padding: 0px;
		margin: 24px 0 0 0;
		float: right;
		width: 24px;
		height: 17px;
	}

	.menu {
		background: #fff;
		width: 100%;
		padding: 30px 0 30px 0;
	}
		
	.nav ul {
		text-align: center;
		padding:0;
		margin-top: 0;
		left: 0;
	}

	.nav ul li {
		float: left;
		width: 100%;
		margin-left: 0;
	}

	.nav ul li a {
		display: block;
		width: 100%;
		padding: 10px 0;
		margin:0px;
	}

	.flex-caption {
		display: none;
	}

	.flex-direction-nav {
		display:none !important;
	}

	.site-content {
		padding: 0;
	}

	.site-content.promo {
		padding: 28px 0 40px;
	}

	.site-content.site-banners {
		padding-top: 20px;
	}

	aside, aside.boxfb {
		margin-bottom: 20px;
	}

	aside.eu {
		margin-top: 20px;
	}

	.shopinfo {
		padding-right:0;
	}

	.site-banners .grid-unit { 
		margin-bottom: 20px;
	}

	.article-image {
		margin-bottom: 10px;
	}

	.article-image a:hover img {
		opacity: 1;
	}

	hr {
		margin: 20px 0 40px;
	}

}

/* Grid
-------------------------------------------------------------- */


@media screen and (min-width: 930px) {
	
	body {
		font-size: 100%;
	}

	.col-2 .grid-unit {
		width: 45%;
		margin-right: 10%;
	}

	.col-2 .grid-unit.left {
		width: 60%;
	}

	.col-2 .grid-unit.right {
		width: 30%;
	}

	.col-2 .grid-unit.article-image {
		width: 30%;
		margin-right: 3%;
	}

	.col-2 .grid-unit.article-content {
		width: 67%;
	}

	.col-2 .grid-unit:last-child, .col-3 .grid-unit:last-child  {
		margin-right: 0;
	}

	.col-3 .grid-unit {
		width: 32%;
		margin-right: 2%;
	}

}

/* Retina background
-------------------------------------------------------------- */

@media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
ul.project-nav li.prev a {
	background: url(img/prev@2x.png) no-repeat center;
	background-size: 60px 60px;
}
ul.project-nav li.index a {
	background: url(img/index@2x.png) no-repeat center;
	background-size: 60px 60px;
}
ul.project-nav li.next a {
	background: url(img/next@2x.png) no-repeat center;
	background-size: 60px 60px;
}
.col-2 .grid-unit.pic img {
	width: 125px;
	height: 125px;
}
}
