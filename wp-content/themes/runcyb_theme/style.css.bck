/* Layout
-------------------------------------------------------------- */

body { 
	margin: 0!important;
	font-size: 100%;
	color: #3b4045;
	font-family: 'Roboto', sans-serif;
	position: relative;
	height: 100%;
	background: rgba(241, 241, 241, 0.96);
}

img, video {
	width: auto;
	max-width: 100%;
	height: auto;
	margin: 0 auto;
}

.container {
	width: 92%;
	margin-left: auto;
	margin-right: auto;
	max-width: 1150px;
	*zoom:1;
}

.grid-container:after {
	content: "";
	display: table;
	clear: both;
}

.grid-row {
	width: 100%;
	overflow: hidden;
}

.grid-row.col-2 {
	margin-bottom: 0;
}

.grid-row.col-4 {
	overflow: visible;
	display: block;
	float: left;
}

.grid-unit {
	position: relative;
	margin-left: 0;
	width: 100%;
	float: left;
	display: inline;
}



.grid:after {
	content: "";
	display: table;
	clear: both;
}

*, *:after, *:before {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

#wrap {
	min-height: 100%;
	overflow:hidden;
	height:100%;
}


/* Header
-------------------------------------------------------------- */

.site-header {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 30;
	-webkit-transition: all 0.5s;
	-moz-transition: all 0.5s;
	transition: all 0.5s;
	background: #fff;
}

.site-header.large {
	padding: 10px 0 10px 0;
	border-bottom: solid #CCC 1px;
}

.page-id-12 .site-header.large { border-bottom: none; }

.site-header.small {
	padding: 5px 0;
	border-bottom: solid #A3A3A3 1px;
	box-shadow: 0px 2px 5px 0 rgba(0,0,0,0.2);

}

.site-header.small .logo {
	width: 200px;
}

.logo {
	float: left;
	width: 240px;
	-webkit-transition: all 0.5s;
	-moz-transition: all 0.5s;
	transition: all 0.5s;
}

a:hover .logo {
	opacity:0.7;
}


/* Main Content Area
-------------------------------------------------------------- */

section {
	float: left;
	width: 100%;
}

.pushdown {
	margin-top: 94px;
}

.site-content {
	padding: 40px 0;;
}

.site-content.homeintro {
	background: #d3d6db;
}

.site-content.site-banners {
	background: #fff;
	margin:0;
	border-bottom:solid 1px #D6D6D6;
}

.site-content.promo {
	background: #ffffff;
	margin:0;
	border-top:solid 1px #D6D6D6;
}

.shadowtop{
	background: url(img/shadow.png) no-repeat center top;
	width:100%;
	height:20px;
	position:relative;
}

.shadowbase{
	background: url(img/shadow_base.png) no-repeat center bottom;
	width:100%;
	height:20px;
	position:relative;
	margin:30;
}


/* Footer
-------------------------------------------------------------- */

.site-footer {
	position: relative;
	padding: 180px 0 0 0;
	background: #11331B url(img/back.jpg) no-repeat top;
	box-shadow: inset 0px 1px 6px 0 rgba(0,0,0,0.3);
}

.site-footer .content img {
	display:inline;
	margin: 0 20px 20px 0;
}

.site-footer span {
	margin-left: 30px;
}

.footertxt {
	padding: 20px 0;
	background: #0e2a16;
}

.footertxt p {
	margin: 0;
	color: rgba(255,255,255,0.5)!important;
	font-weight: bold;
}

.site-footer span.closed {
	font-weight: normal;
	margin-left: 0;
}

.footertxt p a {
	background: none;
	color: rgba(255,255,255,0.5)!important;
}

.footertxt p a:hover {
	color: rgba(255,255,255,1)!important;
	background: none;
}


/* Homepage Banners
-------------------------------------------------------------- */

.site-banners a .title{background:#000000; color:#ffffff; display:inline; padding:10px 10px; width:100%;FLOAT:LEFT;}
.site-banners a:hover .title{bottom:15px; transition: all .3s ease-in-out;
    -webkit-transition: opacity .3s ease-in-out; /** Chrome & Safari **/
    -moz-transition: opacity .3s ease-in-out; /** Firefox **/
    -o-transition: opacity .3s ease-in-out; /** Opera **/
	background:#136431;}
.site-banners img {float:left; display:inline}
.site-banners a:hover img{opacity:.4;
}

.site-banners .title h2{color:#ffffff; font-size:18px;  margin:0;}

.site-banners .title p{ color:#ffffff; font-size:12px; margin:0;}
.bannerhire{position:relative;	z-index:1;}
.bannertrails{position:relative;z-index:1;}
.bannercentre{width:47%; float:left; position:relative;	z-index:1;}
.bannerdyfi{width:47%; float:left; position:relative; z-index:1;margin-right:6%;}


/* Page Content Tabs
-----------------------------------------------------------------*/

.left ul.tabs {
    padding: 15px 0;
    font-size: 0;
    margin:40px 0 20px 0px;
    list-style-type: none;
    text-align: left;
}
        
ul.tabs li {
    display: inline;
    margin: 0;
    margin-right:-1px;
}
        
ul.tabs li a {
    font: bold 14px Roboto;
    text-decoration: none;
    position: relative;
    padding: 12px 3%;
    border: 1px solid #CCC;
    border-bottom-color:#B7B7B7;
    color: #4E4E4E;
    background: #F0F0F0;
    outline:none;
}
        
ul.tabs li a:visited {
    color: #000;
}
        
ul.tabs li a:hover {
    border: 1px solid #B7B7B7;
    background:#DDDDDD;
	color: #000;
}
        
ul.tabs li.selected a, ul.tabs li.selected a:hover {
    position: relative;
    top: 0px;
    background: white;
    border: 1px solid #B7B7B7;
    border-bottom-color: white;
	color: #000;
}
              
ul.tabs li.selected a:hover {
    text-decoration: none;
}
     
div.tabcontents {
    border: 1px solid #B7B7B7; 
    padding: 25px 25px 55px 25px;
    background-color:#FFF;
	margin:-23px 0 20px 0;
}


